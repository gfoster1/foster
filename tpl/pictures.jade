doctype html
html(lang="en")
  head
    title= "Foster.bz Home"
    include includes/header.jade
  body
    div.container
        include includes/navbar.jade
        div#myCarousel(class="carousel slide data-ride=carousel")
            div.carousel-inner(role="listbox")
                div.item.active
                    img(src="http://buymelaughs.com/wp-content/uploads/2013/12/Funny-Inspirational-quotes-7.jpg")
                div.item
                    img(src="http://cameraphotography2015.com/wp-content/uploads/2015/01/funny-photos.jpg")
            a.left.carousel-control(href="#myCarousel", role="button", data-slide="prev")
                span.glyphicon.glyphicon-chevron-left(aria-hidden="true")
                span.sr-only Previous
            a.right.carousel-control(href="#myCarousel", role="button", data-slide="next")
                span.glyphicon.glyphicon-chevron-right(aria-hidden="true")
                span.sr-only Next
        include includes/footer.jade
